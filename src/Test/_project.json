{
    "version": "1.1.0",
    "buildOptions": {
        "emitEntryPoint": true,
        "warningsAsErrors": true
    },
    "configurations": {
        "Debug": {
            "buildOptions": {
                "define": [
                    "DEBUG",
                    "TRACE",
                    "OS_WINDOWS"
                ],
                "debugType": "portable"
            }
        },
        "Release": {
            "buildOptions": {
                "define": [
                    "TRACE",
                    "OS_WINDOWS"
                ],
                "optimize": true
            }
        }
    },
    "dependencies": {
        "Microsoft.NETCore.App": "1.1.0",
        "Microsoft.NETCore.TestHost": "1.1.0",
        "System.Buffers": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0",
        "System.Threading.ThreadPool": "4.3.0",
        "vss-api-netcore": "0.5.30-private",
        "Moq": "4.6.36-alpha",
        "xunit": "2.1.0",
        "xunit.console.netcore": "1.0.3-prerelease-00520-02",
        "xunit.netcore.extensions": "1.0.0-prerelease-00520-01",
        "xunit.runner.utility": "2.1.0",
        "Agent.Listener": {
            "target": "project",
            "version": "1.1.0-*"
        },
        "Microsoft.VisualStudio.Services.Agent": {
            "target": "project",
            "version": "1.1.0-*"
        },
        "Agent.Worker": {
            "target": "project",
            "version": "1.1.0-*"
        }
    },
    "frameworks": {
        "netcoreapp1.1": {
            "imports": [
                "dnxcore50",
                "portable-net451+win8"
            ]
        }
    },
    "runtimes": {
        "win7-x64": {},
        "ubuntu.14.04-x64": {},
        "ubuntu.16.04-x64": {},
        "centos.7-x64": {},
        "rhel.7.2-x64": {},
        "osx.10.11-x64": {}
    }
}